
{% include 'snippets/breadcrumbs.njk' %}

<header>
      {# <a name="{{ title | slugify }}" href="#{{ title | slugify }}"><h1 class="display-1 link-fancy">{{ title }}</h1></a> #}
      {% if icon %}
      <i class="display-1 fa-{{ site.fatheme }} fa-{{ icon }}"></i>
      {% endif %}
      <a name="{{ page.fileSlug }}"><h1 class="display-1 link-fancy">{{ page.fileSlug | slugify({separator:' ', lowercase: false}) | title or title }}</h1></a>
</header>

{{ content | safe }}