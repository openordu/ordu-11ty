  <section class="">
    <div class="container text-center text-md-start mt-5">
      <!-- Grid row -->
      <div class="row mt-3">
        <!-- Grid column -->
        <div class="col-md-3 col-lg-4 col-xl-3 mx-auto mb-4">
          <!-- Content -->
          <h6 class="text-uppercase fw-bold mb-4">
            {{ metadata.title }}
          </h6>
          <p>
            Copyright © 2019-{{ site.currentYear }}, {{ metadata.title }}. {{ metadata.description }} <a href="/license">[Celtic Culture Attribution-ShareAlike 1.0 International Public License]</a>.
          </p>
        </div>

        <!-- Grid column -->
        <div class="col-md-2 col-lg-2 col-xl-2 mx-auto mb-4">
          <!-- Links -->
          <h6 class="text-uppercase fw-bold mb-4">
            Quick Links
          </h6>
          <p>
            <a href="/about" class="text-reset">About Project</a>
          </p>
          <p>
            <a href="/support" class="text-reset">Support Us</a>
          </p>
          <p>
            <a href="/docs" class="text-reset">Documentation</a>
          </p>
          <p>
            <a href="/docs/contributing/conduct" class="text-reset">Code of Conduct</a>
          </p>
          <p>
            <a href="/license" class="text-reset">Open Source Licenses</a>
          </p>
          <p>
            <a href="/FAQs" class="text-reset">Frequently Asked Questions</a>
          </p>
        </div>

        <!-- Grid column -->
        <div class="col-md-3 col-lg-2 col-xl-2 mx-auto mb-4">
          <!-- Links -->
          <h6 class="text-uppercase fw-bold mb-4">
            Social Media
          </h6>
          <p>
            <a href="http://github.com/openordu" target="_blank" class="text-reset">GitHub</a>
          </p>
          <p>
            <a href="#!" class="text-reset">Twitter</a>
          </p>
          <p>
            <a href="https://mastodon.celticpaganism.org" target="_blank" class="text-reset">Mastodon</a>
          </p>
        </div>

        <!-- Grid column -->
        <div class="col-md-4 col-lg-3 col-xl-3 mx-auto mb-md-0 mb-4">
          <!-- Links -->
          <h6 class="text-uppercase fw-bold mb-4">
            Contact
          </h6>
          <p><i class="fas fa-envelope me-3"></i><a class="email-encode" href="mailto:{{ metadata.email }}">{{ metadata.email }}</a></p>

          <h6 class="text-uppercase fw-bold mb-4 mt-4">
            Disclaimer
          </h6>
          <p>
            This project and its contributors are not responsible for any misuse of the software. This project is provided as is and without any warranty.
          </p>
        </div>
      </div>
    </div>
  </section>
  <div class="text-center p-4 bg-primary text-dark">
    Made with ❤️ by contributors around the world
  </div>
