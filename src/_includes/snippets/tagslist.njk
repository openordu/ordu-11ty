{% for tag in collections.tagList %}
  {% set tagUrl %}/tags/{{ tag.tagName | slug }}/{% endset %}
  <a href="{{ '/tags/' | url }}{{ tag.tagName | slug }}" class="btn btn-outline-dark btn-sm me-2 my-2">{{ tag.tagName | lower }}
    <span class="ms-1 badge bg-dark">
    {{ tag.tagCount }}
    </span>
  </a>
{% endfor %}
